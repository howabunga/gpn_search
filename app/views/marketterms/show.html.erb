<div class="page-header">
<h3>Marketing Term #<%= @marketterm.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Flavor</dt>
      <dd><%= @marketterm.flavor.name %></dd>

      <dt>Product</dt>
      <dd><%= @marketterm.product.name %></dd>

      <dt>Name</dt>
      <dd><%= @marketterm.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/marketterms" class="btn btn-primary">
        Back
      </a>
      <a href="/marketterms/<%= @marketterm.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_marketterm/<%= @marketterm.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
